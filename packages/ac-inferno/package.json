{
  "name": "ac-inferno",
  "version": "1.0.6",
  "description": "Elemental components for doing animation",
  "keywords": [
    "react",
    "keyframes",
    "animate",
    "high order component",
    "composed components",
    "animations",
    "styled",
    "merge",
    "delay",
    "element type"
  ],
  "main": "main.js",
  "author": "Nitin Tulswani",
  "license": "MIT",
  "files": [
    "dist",
    "main.js"
  ],
  "homepage": "https://github.com/nitin42/animate-components",
  "scripts": {
    "prepublishOnly": "yarn flow && yarn webpack",
    "start": "./node_modules/.bin/webpack-dev-server --watch --config ./webpack.config.js",
